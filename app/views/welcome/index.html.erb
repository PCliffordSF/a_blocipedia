<div class="container">
    <% @user = current_user %>
    <% if current_user != nil %>
        <p>Welome to Blocipedia <%= @user.email %></p>
    
  <div class="col-md-6">
    <p>Would you like to create a new Wiki?</p>
     <div>
       <%= link_to "Create Wiki", new_wiki_path, class: 'btn btn-success' %>
     </div>
     <p>Would you like to view your Wikis?</p>
    <div>
       <%= link_to "All My Wikis", wikis_path, class: 'btn btn-success' %>
     </div>
  <div>
      <% else %>
    <div class="col-md-4">
        <p>Would you like to login or sign up?</p>
    </div>  
      <% end %>
</div>