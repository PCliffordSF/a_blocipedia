 <h1>All Wikis</h1>

 <div class="row">
   <div class="col-md-8">

     <% @wikis.each do |wiki| %>
       <div class="media">
         <div class="media-body">
           <h4 class="media-heading">

             <%= link_to wiki.title, wiki %>
           </h4>
           <small>
             <%= wiki.body %> </br>
           </small>
           <small>
             <%= link_to "Edit Wiki", edit_wiki_path(wiki), class: 'btn-xs btn-success' %>
  
           </small>
         </div>
       </div>
     <% end %>
     </br>
     <%= link_to "Exit", welcome_index_path, class: 'btn btn-warning' %>
   </div>
 </div>